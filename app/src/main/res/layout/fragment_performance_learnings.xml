                android:text="98.4"
                android:textColor="#9163D7"
                android:textSize="@dimen/_8sdp"
                app:fontFamily="@font/lato_semi_bold"
                app:layout_constraintBottom_toBottomOf="@id/stuContainer2"
                app:layout_constraintEnd_toEndOf="@+id/stuContainer2"

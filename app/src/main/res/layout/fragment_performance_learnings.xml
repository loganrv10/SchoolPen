                app:layout_constraintBaseline_toBaselineOf="@id/tvNatasha"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/tvNatasha" />

            <com.google.android.material.card.MaterialCardView
